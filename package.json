{
  "name": "saturn-service",
  "version": "4.3.0",
  "description": "The Saturn service is an API used for financial organization",
  "scripts": {
    "start": "node -r module-alias/register ./dist/main/server.js",
    "start:dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --watch src ./src/main/server.ts",
    "start:debug": "nodemon -L --watch ./dist -r module-alias/register --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "build": "rimraf dist && tsc -p tsconfig-build.json -w",
    "test": "jest --passWithNoTests --silent --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit.config.js",
    "test:e2e": "npm test -- --watch -c jest-e2e.config.js",
    "test:ci": "npm test -- --coverage",
    "test:staged": "jest --passWithNoTests --silent --noStackTrace --findRelatedTests",
    "lint": "eslint src/**/*.ts && eslint --fix",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@shelf/jest-mongodb": "^4.3.2",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^22.0.0",
    "@types/supertest": "^6.0.2",
    "@types/validator": "^13.12.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-love": "^62.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "git-commit-msg-linter": "^5.0.8",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "mockdate": "^3.0.5",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "sucrase": "^3.35.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mongodb": "^6.8.0",
    "nodemon": "^3.1.4",
    "validator": "^13.12.0"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
